{
  "name": "memdown",
  "version": "3.0.0",
  "description": "An drop-in replacement for LevelDOWN that works in memory only",
  "authors": [
    "Rod Vagg <rod@vagg.org> (https://github.com/rvagg)"
  ],
  "license": "MIT",
  "main": "memdown.js",
  "scripts": {
    "test": "standard && nyc node test.js",
    "test-browsers": "airtap --loopback airtap.local --coverage test.js",
    "test-browser-local": "airtap --coverage --local test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "files": [
    "memdown.js",
    "immediate.js",
    "immediate-browser.js",
    "UPGRADING.md"
  ],
  "browser": {
    "./immediate.js": "./immediate-browser.js"
  },
  "dependencies": {
    "abstract-leveldown": "~5.0.0",
    "functional-red-black-tree": "~1.0.1",
    "immediate": "~3.2.3",
    "inherits": "~2.0.1",
    "ltgt": "~2.2.0",
    "safe-buffer": "~5.1.1"
  },
  "devDependencies": {
    "airtap": "^2.0.0",
    "coveralls": "^3.0.2",
    "hallmark": "^0.1.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "standard": "^12.0.0",
    "tape": "^4.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level/memdown.git"
  },
  "homepage": "https://github.com/Level/memdown",
  "keywords": [
    "level",
    "leveldb",
    "leveldown",
    "levelup",
    "memory"
  ],
  "engines": {
    "node": ">=6"
  }
}
